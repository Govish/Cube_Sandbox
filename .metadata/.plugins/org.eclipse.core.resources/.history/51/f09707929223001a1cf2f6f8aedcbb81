#ifndef STUSB4500_H
#define STUSB4500_H




// =========== Public Functions ============

HAL_StatusTypeDef pd_init();
HAL_StatusTypeDef pd_soft_reset();	//call to soft-reset the STUSB4500
HAL_StatusTypeDef pd_send_soft_reset();	//call to send a soft-reset on the active CC line

HAL_StatusTypeDef pd_read_sink_pdos(uint8_t* num_pdos, uint16_t* pdos); //read how many pdo's we have set up and what they are

#endif
